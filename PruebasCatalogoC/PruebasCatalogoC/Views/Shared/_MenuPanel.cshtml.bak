                            });
                            $('div [class=menuSection]').each(function (){
                                var span = $(this).find('span');
                                if(span.length==0){
                                    $(this).remove();
                                }
                            });
                        }
                    }
                });

                $.ajax({
                    type: 'POST',
                    url: '@Url.Action("GetAccionesNegadas", "Cuenta", new { area = "" })',
                    async: false,
                    dataType: 'json',
                    validaAcciones: true,
                    success: function (data) { accionesNegadas = data; }
                });

                $('#dvMenuPanel').show();
            });
   
        </script>

    }
    